
UE1ASM  = python3 ue1asm.py
UE1PACK = python3 ue1-ttl-pack.py

PROGRAMS = \
	UE1FIBO.BIN \
	UE1MATH.BIN \
	UE1_DIAPER1_V2.BIN \
	UE1_DIAPER2_V1.BIN \
	MUL5X5.BIN \
	DIV8X4.BIN \
	UNUSED.BIN

all: UE1TEST.ROM

UE1TEST.ROM: $(PROGRAMS)
	$(UE1PACK) UE1TEST.ROM $(PROGRAMS) UNUSED.BIN

UE1FIBO.BIN: UE1FIBO.ASM
	$(UE1ASM) UE1FIBO.ASM UE1FIBO.BIN

UE1MATH.BIN: UE1MATH.ASM
	$(UE1ASM) UE1MATH.ASM UE1MATH.BIN

UE1_DIAPER1_V2.BIN: UE1_DIAPER1_V2.ASM
	$(UE1ASM) UE1_DIAPER1_V2.ASM UE1_DIAPER1_V2.BIN

UE1_DIAPER2_V1.BIN: UE1_DIAPER1_V2.ASM
	$(UE1ASM) UE1_DIAPER2_V1.ASM UE1_DIAPER2_V1.BIN

MUL5X5.BIN: MUL5X5.ASM
	$(UE1ASM) MUL5X5.ASM MUL5X5.BIN

DIV8X4.BIN: DIV8X4.ASM
	$(UE1ASM) DIV8X4.ASM DIV8X4.BIN

UNUSED.BIN: UNUSED.ASM
	$(UE1ASM) UNUSED.ASM UNUSED.BIN

clean:
	rm -f *.BIN *.ROM
